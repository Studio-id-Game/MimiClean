{
  "metadata": [
    {
      "src": [
        {
          "src": "../MimiClean",
          "files": [
            "**/*.sln"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": true
    }
  ],
  "build": {
    "globalMetadata": {
      "_appName": "StudioIdGames MimiClean document",
      "_appTitle": "StudioIdGames MimiClean document",
      "_appFooter": "<span style='float: left;'> Copyright Â© 2015-2017 StudioIdGames </span> <span style='float: right; margin-right: 50px;' > Generated by <strong>DocFX </strong></span>",
      "_enableSearch": true
    },
    "sitemap": {
      "baseUrl": "https://studio-id-game.github.io/MimiClean/"
    },
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "overwriteFiles": [
      "overwrites/**.md"
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md",
          "namespaces/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "src": "../images",
        "files": [
          "**"
        ],
        "dest": "images"
      },
      {
        "files": [
          "logo.svg",
          "favicon.ico",
          "googlec803c82e9c3c4f22.html",
          "robots.txt"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "templates/unity"
    ],
    "xref": [
    ],
    "xrefService": [
        "https://xref.docs.microsoft.com/query?uid={uid}"
    ]
  }
}
